$custom_agrupamento = Get-SCCustomProperty -VMMServer "{{ servidor_vmm }}" -Name "{{ campo_agrupamento }}"
$custom_nome_id = Get-SCCustomProperty -VMMServer "{{ servidor_vmm }}" -Name "{{ campo_id }}"
$custom_imagem = Get-SCCustomProperty -VMMServer "{{ servidor_vmm }}" -Name "{{ campo_imagem }}"
$custom_regiao = Get-SCCustomProperty -VMMServer "{{ servidor_vmm }}" -Name "{{ campo_regiao }}"
$custom_rede_principal = Get-SCCustomProperty -VMMServer "{{ servidor_vmm }}" -Name "{{ campo_rede_principal }}"
$vm = Get-SCVirtualMachine -VMMServer "{{ servidor_vmm }}" -Name "{{ vm_name }}"

Set-SCCustomPropertyValue -InputObject $vm -CustomProperty $custom_agrupamento -Value "{{ group }}"
Set-SCCustomPropertyValue -InputObject $vm -CustomProperty $custom_nome_id -Value "{{ vm_name }}"
Set-SCCustomPropertyValue -InputObject $vm -CustomProperty $custom_imagem -Value "{{ image }}"
Set-SCCustomPropertyValue -InputObject $vm -CustomProperty $custom_regiao -Value "{{ region }}"
Set-SCCustomPropertyValue -InputObject $vm -CustomProperty $custom_rede_principal -Value "{{ network_default }}"
